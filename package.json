{
  "name": "vue3-tx-component",
  "version": "0.1.0",
  "description": "一套基于Vue3的超级实用的前端特效组件库",
  "author": "TX Team",
  "license": "MIT",
  "type": "module",
  "main": "dist/vue3-tx-component.umd.js",
  "module": "dist/vue3-tx-component.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue3-tx-component.es.js",
      "require": "./dist/vue3-tx-component.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "files": [
    "dist",
    "src/types",
    "README.md"
  ],
  "keywords": [
    "vue",
    "vue3",
    "component",
    "ui-components",
    "effects",
    "animation",
    "transitions",
    "typescript"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build:lib": "vue-tsc && vite build",
    "preview": "vite preview",
    "prepare": "npm run build:lib",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/vue3-tx-component.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/vue3-tx-component/issues"
  },
  "homepage": "https://github.com/yourusername/vue3-tx-component#readme",
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.17.46",
    "@vitejs/plugin-vue": "^4.4.0",
    "terser": "^5.21.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.9.1",
    "vitepress": "^1.0.0-rc.25",
    "vue-tsc": "^1.8.5"
  }
}
